import pandas as pd

def createDataframe(student_data):
    df = pd.DataFrame(student_data, columns=["student_id", "age"])
    return df
if __name__ == "__main__":
    student_data = [
        [101, 25],
        [102, 24],
        [103, 25],
        [104, 26],
        [105, 23]
    ]
    result_table = createDataframe(student_data)
    print (result_table)
